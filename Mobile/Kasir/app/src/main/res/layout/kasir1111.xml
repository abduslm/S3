<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F1F1F1"
    tools:context=".YourActivity">

    <LinearLayout
        android:id="@+id/header_bar"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingStart="32dp"
        android:paddingEnd="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/logo_kedai_temeji"
                android:layout_width="48dp"
                android:layout_height="58dp"
                android:contentDescription="Logo Kedai Temeji"
                android:scaleType="fitCenter"
                android:src="@drawable/logokedaitemeji" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/oneway"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/calibri_regular"
                    android:gravity="start"
                    android:text="Kedai Temeji"
                    android:textSize="20sp"
                    />

                <TextView
                    android:id="@+id/jum_at_19_s"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:text="Jumâ€™at 19-September-2025 / Mahesa"
                    android:textSize="9sp" />
            </LinearLayout>
        </LinearLayout>

        <androidx.cardview.widget.CardView
            android:id="@+id/search_card"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:layout_marginStart="100dp"
            app:cardBackgroundColor="#FFFFFF"
            app:cardCornerRadius="20dp"
            app:cardElevation="1dp">

            <EditText
                android:id="@+id/et_search"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="24dp"
                android:background="@android:color/transparent"
                android:drawableStart="@drawable/cari"
                android:drawablePadding="8dp"
                android:hint="Search"
                android:inputType="text"
                android:textSize="14sp" />
        </androidx.cardview.widget.CardView>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/ordermenu4"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:elevation="8dp"
                android:scaleType="fitCenter"
                android:src="@drawable/ordermenu"
                android:contentDescription="Order Menu Icon" />

            <TextView
                android:id="@+id/ordermenu5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="4dp"
                android:fontFamily="@font/manrope_bold"
                android:text="Order Menu" />
        </LinearLayout>

        <Button
            android:id="@+id/daftar_pesanan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="400dp"
            android:background="@drawable/daftarpesanan"
            android:drawableStart="@drawable/edit"
            android:drawablePadding="5dp"
            android:fontFamily="@font/manrope_bold"
            android:gravity="center"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"
            android:text="Daftar Pesanan"
            app:backgroundTint="@null" />

    </LinearLayout>

    <View
        android:id="@+id/header_separator"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#CCCCCC"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/header_bar" />

    <View
        android:id="@+id/rectangle_2"
        android:layout_width="168dp"
        android:layout_height="0dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/header_separator" />

    <TextView
        android:id="@+id/minuman1"
        android:layout_width="104dp"
        android:layout_height="28dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="8dp"
        android:gravity="top"
        android:text="Minuman"
        android:textColor="#844B16"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/header_separator"
        tools:ignore="RtlSymmetry" />

    <ImageView
        android:id="@+id/vector"
        android:layout_width="10dp"
        android:layout_height="6dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="22dp"
        android:scaleType="fitCenter"
        android:src="@drawable/vector"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/header_separator"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/pop_ice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="8dp"
        android:text="Pop Ice"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/minuman1" />

    <TextView
        android:id="@+id/marimas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="8dp"
        android:text="Marimas"
        android:textColor="#4D4D4D"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pop_ice" />

    <TextView
        android:id="@+id/teajus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="8dp"
        android:text="Teajus"
        android:textColor="#4D4D4D"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/marimas" />

    <TextView
        android:id="@+id/nutrisari"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="8dp"
        android:text="Nutrisari"
        android:textColor="#4D4D4D"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/teajus" />

    <TextView
        android:id="@+id/gooday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="8dp"
        android:text="Goodday"
        android:textColor="#4D4D4D"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nutrisari" />

    <TextView
        android:id="@+id/maxtea"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="8dp"
        android:text="Maxtea"
        android:textColor="#4D4D4D"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gooday" />

    <TextView
        android:id="@+id/kopi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="8dp"
        android:text="Kopi"
        android:textColor="#4D4D4D"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/maxtea" />

    <TextView
        android:id="@+id/milo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="8dp"
        android:text="Milo"
        android:textColor="#4D4D4D"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kopi" />

    <TextView
        android:id="@+id/dancow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="8dp"
        android:text="Dancow"
        android:textColor="#4D4D4D"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/milo" />

    <TextView
        android:id="@+id/background"
        android:layout_width="140dp"
        android:layout_height="41.6dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="375dp"
        android:background="@drawable/background_kasir1"
        android:gravity="center"
        android:paddingTop="1.6dp"
        android:text="Kembali"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dancow" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/content_layout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/rectangle_2"
        app:layout_constraintTop_toBottomOf="@+id/header_separator">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:background="@color/black">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="65"
                android:orientation="vertical"
                android:background="#F1F1F1">

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="35"
                android:orientation="vertical"
                android:background="#FFFFFF">

                <!-- Auto layout, grids, variables, and unit scale are not yet supported -->
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cart_items_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="20dp"
                    android:paddingStart="14dp">

                    <View
                        android:id="@+id/rendang"
                        android:layout_width="300dp"
                        android:layout_height="95dp"
                        android:background="@drawable/indomierendangxml"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        />

                    <View
                        android:id="@+id/rectangle_8"
                        android:layout_width="21.29dp"
                        android:layout_height="0dp"
                        android:background="@drawable/indomie3"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/rendang"
                        app:layout_constraintBottom_toBottomOf="@+id/rendang" />

                    <View
                        android:id="@+id/goreng"
                        android:layout_width="300dp"
                        android:layout_height="95dp"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/indomierendangxml"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/rendang" />

                    <View
                        android:id="@+id/rectangle_9"
                        android:layout_width="21.29dp"
                        android:layout_height="0dp"
                        android:background="@drawable/indomie3"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/goreng"
                        app:layout_constraintBottom_toBottomOf="@+id/goreng" />

                    <View
                        android:id="@+id/esteh"
                        android:layout_width="300dp"
                        android:layout_height="95dp"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/indomierendangxml"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/goreng" />

                    <View
                        android:id="@+id/rectangle_10"
                        android:layout_width="21.29dp"
                        android:layout_height="0dp"
                        android:background="@drawable/indomie3"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/esteh"
                        app:layout_constraintBottom_toBottomOf="@+id/esteh" />
                    <TextView
                        android:id="@+id/mierendang1"
                        android:layout_width="154dp"
                        android:layout_height="26dp"
                        android:layout_marginStart="80dp"
                        android:layout_marginTop="16dp"
                        android:text="Indomie Rendang"
                        android:textAppearance="@style/indomierendang"
                        android:gravity="top"
                        android:fontFamily="@font/manrope_bold"
                        app:layout_constraintStart_toStartOf="@+id/rendang"
                        app:layout_constraintTop_toTopOf="@+id/rendang"/>

                    <View
                        android:id="@+id/top_view_de"
                        android:layout_width="37dp"
                        android:layout_height="48dp"
                        android:layout_marginStart="30dp"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/indomierendang"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                    <TextView
                        android:id="@+id/rp_10_000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Rp.10.000"
                        android:textAppearance="@style/hargaindomierendang"
                        android:gravity="top"

                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintHorizontal_bias="0.9"
                        app:layout_constraintVertical_bias="0.2"
                        android:layout_marginTop="40dp"
                        android:layout_marginStart="79dp"
                        />
                    <ImageView
                        android:id="@+id/iconsax_bro"
                        android:layout_width="14dp"
                        android:layout_height="14dp"

                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginTop="60dp"
                        android:layout_marginEnd="282dp"
                        android:src="@drawable/editabu"
                        android:scaleType="fitCenter"

                        android:clipToOutline="true"
                        />
                    <TextView
                        android:id="@+id/keterangan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Jangan terlalu pedas"
                        android:textAppearance="@style/ketmierendang"
                        android:gravity="top"

                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginTop="58dp"
                        android:layout_marginEnd="166dp"
                        />
                    <ImageView
                        android:id="@+id/minus"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:background="@drawable/indomie4"
                        android:src="@drawable/gariskecil"
                        android:padding="5dp"

                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginTop="40dp"
                        android:layout_marginEnd="128dp"
                        />
                    <ImageView
                        android:id="@+id/plus"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:background="@drawable/indomie5"
                        android:src="@drawable/garistambah"
                        android:padding="5dp"

                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginTop="40dp"
                        android:layout_marginEnd="90dp"
                        />
                    <TextView
                        android:id="@+id/satu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="1"
                        android:textAppearance="@style/satu"
                        android:gravity="top"

                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintHorizontal_bias="0.9"
                        app:layout_constraintVertical_bias="0.2"
                        android:layout_marginTop="37dp"
                        android:layout_marginStart="250dp"
                        />


                </androidx.constraintlayout.widget.ConstraintLayout>

            </LinearLayout>

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>





